import java.lang.Math;

public class maf
{
    public static void main (String[] args)
    {
        int x = 11; //when using random numbers like this, always use a higher number than your intended max
        double y = Math.random();
        int z =  (int) (x * y);
        
        System.out.println("A random number between 0 and 10 is: " + z);
    }
} //noah triche per 5
